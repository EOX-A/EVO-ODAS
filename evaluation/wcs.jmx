              <stringProp name="script">String axisX = vars.get(&quot;x_axis_name&quot;);
String axisY = vars.get(&quot;y_axis_name&quot;);

String subsetX = vars.get(&quot;subset_x&quot;);
String subsetY = vars.get(&quot;subset_y&quot;);
String sizeX = vars.get(&quot;size_x&quot;);
String sizeY = vars.get(&quot;size_y&quot;);

Arguments args = sampler.getArguments();

if (subsetX.length()) {
	args.addArgument(&quot;subset&quot;, subsetX);
}
if (subsetY.length()) {
	args.addArgument(&quot;subset&quot;, subsetY);
}

ArrayList sizes = new ArrayList();
if (sizeX.length()) {
	sizes.add(axisX + &quot;(&quot; + sizeX + &quot;)&quot;);
}
if (sizeY.length()) {
	sizes.add(axisY + &quot;(&quot; + sizeY + &quot;)&quot;);
}

if (sizes.length()) {
	args.addArgument(&quot;scaleSizes&quot;, String.join(&quot;,&quot;, sizes));
}

</stringProp>
